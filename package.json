{
  "name": "mission-02",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "cross-env VITE_USE_MOCK=true vite",
    "dev:api": "cross-env VITE_USE_MOCK=false vite",
    "build": "vue-tsc && vite build",
    "build:mock": "cross-env VITE_USE_MOCK=true vue-tsc && vite build",
    "build:prod": "cross-env VITE_USE_MOCK=false vue-tsc && vite build",
    "preview": "vite preview",
    "preview:mock": "cross-env VITE_USE_MOCK=true vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "screenshots:clean": "rm -rf screenshots/*"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@tanstack/vue-query": "^5.74.6",
    "@types/axios": "^0.14.4",
    "ant-design-vue": "4.0.0-rc.6",
    "axios": "^1.10.0",
    "lodash": "^4.17.21",
    "pinia": "^2.3.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.5.0",
    "yup": "^1.6.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@storybook/addon-docs": "^9.0.14",
    "@storybook/vue3-vite": "^9.0.14",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.16.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.4.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^9.0.14",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.2",
    "prettier": "^2.8.8",
    "storybook": "^9.0.14",
    "typescript": "^5.5.4",
    "vite": "^5.4.18",
    "vitest": "^1.6.1",
    "vue-tsc": "^2.0.0"
  }
}
